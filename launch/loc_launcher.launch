<launch>
	<param name="use_sim_time" value="true"/>
	<node pkg = "tf" type="static_transform_publisher" name = "rearGPS" args="0 0 0 0 0 0 1 car rgps 100"/>
	<node pkg = "tf" type="static_transform_publisher" name = "rearIMU" args="0 0 0 0 0 0 1 car rimu 100"/>	
	<node pkg = "tf" type="static_transform_publisher" name = "static_map" args="0 0 0 0 0 0 1 world map 100"/>
	<node pkg="ros_project" type="ackerman_node" name="ackerman_node" />
	<node pkg="imu_complementary_filter" type = "complementary_filter_node" name = "complementary_filter_gain_node" output="screen">
		<remap from="/imu/data_raw" to="/swiftnav/rear/imu"/>
		<remap from="/imu/mag" to="/swiftnav/rear/magnetometer"/>		
		<param name = "do_bias_estimation" value = "true"/>
		<param name = "do_adaptive_gain" value = "true"/>
		<param name = "use_mag" value = "true"/>
		<param name = "gain_acc" value = "0.01"/>
		<param name = "gain_mag" value = "0.01"/>
	</node>
	<node pkg="robot_localization" type="navsat_transform_node" name="gps_node" output="screen" clear_params="true">
		<remap from = "/gps/fix" to="/swiftnav/rear/gps"/>
		<param name = "frequency" value="30"/>
		<param name = "magnetic_declination_radians" value = "0.048578"/>
		<param name = "yaw_offset" value="2.775"/>
		<param name = "zero_altitude" value="true"/>
		<param name = "use_odometry_yaw" value="true"/>
		<param name = "publish_filtered_gps" value="true"/>
		<param name = "broadcast_utm_transform" value="true"/>
	</node>
	<node pkg="robot_localization" type="ekf_localization_node" name="ekf" clear_params="true">
		<param name = "frequency" value = "30"/>
		<param name = "two_d_mode" value = "true"/>
		<param name = "world_frame" value = "world"/>
		<param name = "odom_frame" value = "world"/>
		<param name = "base_link_frame" value = "car"/>
		<param name = "odom0" value="odometry/ackerman"/>
		<rosparam param = "odom0_config">[true,true,false,false,false,true,false,false,false,false,false,false,false,false,false]</rosparam>
		<param name = "odom0_differential" value = "false"/>
		<param name = "imu_rear" value="imu/data"/>
		<param name = "imu_rear_differential" value = "false"/>
		<rosparam param = "imu_rear_config">[false,false,false,true,true,true,false,false,false,true,true,true,true,true,true]</rosparam>
	</node>-->
</launch>